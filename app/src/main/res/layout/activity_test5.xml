                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <RadioButton
                    android:id="@+id/q10op1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="The Wind"
                    android:textSize="20sp" />

                <RadioButton
                    android:id="@+id/q10op2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="A Flag"
                    android:textSize="20sp" />

                <RadioButton
                    android:id="@+id/q10op3"
