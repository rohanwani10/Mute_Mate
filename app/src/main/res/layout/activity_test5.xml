            <RadioGroup
                android:id="@+id/t1q4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <RadioButton
                    android:id="@+id/q4op1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="to watch"
                    android:textSize="20sp" />

                <RadioButton
                    android:id="@+id/q4op2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="to write"
                    android:textSize="20sp" />

                <RadioButton
                    android:id="@+id/q4op3"
                    android:layout_width="match_parent"
